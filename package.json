{
  "name": "next-auth-adapter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/nanoid-dictionary": "^4.2.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-react": "^7.27.1",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "prisma": "^3.11.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "type-fest": "^2.12.0",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "-- preset": "# Run preset from @thesunny/presets",
    "preset": "ts-node --project node_modules/@thesunny/presets/tsconfig.ts-node.json node_modules/@thesunny/presets/bin/index.ts",
    "-- fix": "# fix syntax",
    "fix:prettier": "yarn lint:prettier --write",
    "-- lint": "# lint syntax",
    "lint:prettier": "prettier --check \"**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:tsc",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --build ./ --force",
    "-- ts-node": "# run with ts-node",
    "ts": "ts-node --project tsconfig.ts-node.json",
    "-- Knex": "# Knex migrations",
    "knex": "ENV=.env/dev.env ts-node --project tsconfig.ts-node.json ./node_modules/.bin/knex",
    "migrate:make": "yarn knex migrate:make",
    "migrate:up": "yarn knex migrate:up",
    "migrate:down": "yarn knex migrate:down",
    "migrate:latest": "yarn knex migrate:latest",
    "migrate:rollback": "yarn knex migrate:rollback",
    "migrate:list": "yarn knex migrate:list",
    "migrate:zero": "yarn knex migrate:rollback --all",
    "remigrate": "yarn migrate:zero && yarn migrate:latest && yarn regenerate",
    "seed": "yarn knex seed:run",
    "--- test": "#",
    "test:once": "jest --config=jest.config.js",
    "test:watch": "jest --watch --config=jest.fast.config.js",
    "test:watch:types": "jest --watch --config=jest.config.js",
    "test:clearcache": "jest --clearCache",
    "-- next": "# NextJs",
    "start:dev": "DOTENV=.env/dev.env next dev -p 3001",
    "-- Prism": "# Prism",
    "prisma": "dotenv -e .env/dev.env -- npx prisma",
    "regenerate": "yarn prisma db pull && yarn prisma generate",
    "-- end": "# end"
  },
  "dependencies": {
    "@emotion/core": "^10",
    "@emotion/styled": "^10",
    "@prisma/client": "3.11.0",
    "@thesunny/assert-type": "^0.1.7",
    "@thesunny/get-env": "^1.0.1",
    "dotenv": "^16.0.0",
    "knex": "^1.0.4",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.1",
    "nanoid-dictionary": "^4.3.0",
    "next": "^12.1.0",
    "next-auth": "^4.2.1",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "superstruct": "^0.15.4",
    "unique-username-generator": "^1.0.1"
  }
}
